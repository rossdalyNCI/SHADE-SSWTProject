<div class="itemsContainer">
    <h1>You Looked for these items</h1>
    
    <% @items.each do |item| %>
    
    <div class="itemhols">
        <h1><%= item.title %></h1>
        <%= image_tag item.image_url, :class=>"itemholsIm" %>
    
        <p>This item is in the category: <%= item.category %></p>
        
    </div>
    <% end %>
    
</div>